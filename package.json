{
  "name": "react-component-generator",
  "version": "1.0.0",
  "description": "With the react-component-generator you can easily generate new react components. This generator also allows you to choose between Typescript and Javascript.",
  "main": "index.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare" : "npm run build",
    "prepublishOnly" : "npm test && npm run lint",
    "preversion" : "npm run lint",
    "version" : "npm run format && git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "gitlink"
  },
  "keywords": [
    "react",
    "generate",
    "component"
  ],
  "author": "Alexander Gerlach",
  "license": "ISC",
  "bugs": {
    "url": "gitlink"
  },
  "homepage": "gitlink",
  "devDependencies": {
    "@types/exit": "^0.1.31",
    "@types/graceful-fs": "^4.1.3",
    "@types/jest": "^25.2.1",
    "@types/prompts": "^2.0.5",
    "exit": "^0.1.2",
    "graceful-fs": "^4.2.3",
    "jest": "^25.3.0",
    "prettier": "^2.0.4",
    "prompts": "^2.3.2",
    "ts-jest": "^25.4.0",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
}
