{
  "name": "react-component-generator",
  "version": "1.0.0",
  "description": "With the react-component-generator you can easily generate new react components. This generator also allows you to choose between Typescript and Javascript.",
  "main": "./src/index.js",
  "bin": {
    "generate-react-component": "./lib/index.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./node_modules ./package-lock.json && npm install",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/P1NHE4D/build-react-component.git"
  },
  "keywords": [
    "react",
    "generate",
    "component"
  ],
  "author": "Alexander Gerlach",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/P1NHE4D/build-react-component.git"
  },
  "homepage": "https://github.com/P1NHE4D/build-react-component.git",
  "dependencies": {
    "commander": "^5.0.0",
    "exit": "^0.1.2",
    "graceful-fs": "^4.2.3",
    "prompts": "^2.3.2"
  },
  "devDependencies": {
    "@types/exit": "^0.1.31",
    "@types/graceful-fs": "^4.1.3",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/prompts": "^2.0.5",
    "jest": "^25.3.0",
    "prettier": "^2.0.4",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}