{
  "name": "generate-react-components",
  "version": "1.0.4",
  "description": "Allows you to easily generate new react components. Features: - Typescript and Javascript support - SCSS, SASS, and CSS support - Multiple component generation",
  "main": "./src/index.js",
  "types": "./lib/index.d.ts",
  "bin": {
    "generate-react-components": "./lib/index.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./node_modules ./package-lock.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --silent --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/P1NHE4D/generate-react-components"
  },
  "keywords": [
    "react",
    "generate",
    "component",
    "create",
    "typescript",
    "javascript"
  ],
  "author": "Alexander Gerlach",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/P1NHE4D/generate-react-components"
  },
  "homepage": "https://github.com/P1NHE4D/generate-react-components",
  "dependencies": {
    "commander": "^5.0.0",
    "exit": "^0.1.2",
    "graceful-fs": "^4.2.3",
    "log-symbols": "^3.0.0",
    "prompts": "^2.3.2"
  },
  "devDependencies": {
    "@types/exit": "^0.1.31",
    "@types/graceful-fs": "^4.1.3",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/prompts": "^2.0.5",
    "fs-extra": "^9.0.0",
    "jest": "^25.3.0",
    "kleur": "^3.0.3",
    "mock-fs": "^4.11.0",
    "prettier": "^2.0.4",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}
